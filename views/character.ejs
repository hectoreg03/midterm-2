<%- include("partials/header", {charname: char.firstName}); %>
<div class=" justify-content-center">
        <img id="demo2" src= "https://thronesapi.com/assets/images/<%= char.image %>"" width="300"  height="300">
    <div >
      <div > 
        <h3>    
        <%=char.fullName %>
        </h3>
    </div>
    <div >
        <div > 
          <h5>    
          Title:
          </h5>
          <ul>
          <%  for(var k in complement.titles) { %>
              <li><%= complement.titles[k]%></li>
          <% } %>
        </ul>
          <br>
          <h5>    
            Aliases:
            </h5>
            
          <ul>
            <%  for(var k in complement.aliases) { %>
                <li><%= complement.aliases[k]%></li>
            <% } %>
          </ul>
            <br>
            <h5>    
              Family:
              </h5>
              <%= char.family%>
              <br>
              <h5>    
                Born:
                </h5>
                <%= complement.born%>
                <br>
                <h5>    
                  Death:
                  </h5>
                  <%  if(complement.died == '') { %>
                    -
                  <% } else{%>
                    <%= complement.died%>
                    <% } %>
                  <br>
        </div>
    </div>
</div>
<br/>
<form action="/character" method="post">
    <input type="submit" class="btn btn-dark" name="previous" value="Previous" />
    <input type="submit" class="btn btn-dark" name="next" value="Next" />
</form>
<%- include("partials/footer"); %>